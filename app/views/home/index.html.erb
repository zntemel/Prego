<script type="text/javascript">
    var cityButtons = document.getElementsByClassName('citybutton');
    for (var i=0; i<20; i++){
        cityButtons[i].addEventListener("mouseout", function(event){
            controlActiveCities(event);
        });
    }
    function controlActiveCities(event) {
        var selectedCities = document.getElementsByClassName('citybutton active');
        // console.log(document.getElementsByClassName('citybutton active'));
        if (selectedCities.length > 7){
            var targetElement = event.target || event.srcElement;
            // console.log(targetElement.classList);
            targetElement.classList.remove("active");
            alert('you cannot select more than 7 cities!');
        }
    }

    window.consistent = true;

    function sendRequest(selectedCities){
        var selectedCityInitials = [];
        for(i = 0; i < selectedCities.length; i++) {
            var initial = selectedCities[i].querySelectorAll("h5")[0].id;
            selectedCityInitials.push(initial);
        }

        var http = new XMLHttpRequest();
        var url = "/results";

        var params = {
            'eigenVector' : window.eigenVector,
            'cityInitials' : selectedCityInitials
        };

        http.open("POST", url, true);

        //Send the proper header information along with the request
        http.setRequestHeader("Content-type", "application/json");

        http.onreadystatechange = function() {//Call a function when the state changes.
            if(http.readyState == 4 && http.status == 200) {
                alert(http.responseText);
            }
        }
        http.send(JSON.stringify(params));
    }

    function controlMinActiveCitiesAndConsistency(selectedCities){
        if (selectedCities.length < 2 && window.consistent == false){
            alert('you should choose more than one city and your data is inconsistent');
        } else if (selectedCities.length < 2){
            alert('you should choose more than one city');
        } else if (window.consistent == false){
            alert('your data is inconsistent');
        } else {
            sendRequest(selectedCities);
        }
    }

    // debugger;
    var searchCriteriaButton = document.getElementById('search-criteria-button');
    searchCriteriaButton.addEventListener("click", function(){
        var selectedCities = document.getElementsByClassName('citybutton active');
        controlMinActiveCitiesAndConsistency(selectedCities);
    });
</script>

