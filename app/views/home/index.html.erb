<script type="text/javascript">
    function updateCriteriaMatrix(criteriaNumber, sliderValue) {
        if (criteriaNumber === '1'){
            var leftCriteria = 'cityRating'
            var rightCriteria = 'flightPricing';
        } else if (criteriaNumber === '2'){
            var leftCriteria = 'cityPricing'
            var rightCriteria = 'flightPricing';
        } else if (criteriaNumber === '3'){
            var leftCriteria = 'visitFrequency'
            var rightCriteria = 'flightPricing';
        } else if (criteriaNumber === '4'){
            var leftCriteria = 'cityPricing'
            var rightCriteria = 'cityRating';
        } else if (criteriaNumber === '5'){
            var leftCriteria = 'visitFrequency'
            var rightCriteria = 'cityRating';
        } else if (criteriaNumber === '6'){
            var leftCriteria = 'visitFrequency'
            var rightCriteria = 'cityPricing';
        }


        if (criteriaNumber === criteriaNumber){
            if (sliderValue > 0){
                criteriaMatrix[rightCriteria][leftCriteria] = sliderValue + 1;
                criteriaMatrix[leftCriteria][rightCriteria] = 1 / (sliderValue + 1);
            } else if (sliderValue === 0){
                criteriaMatrix[rightCriteria][leftCriteria] = 1;
                criteriaMatrix[leftCriteria][rightCriteria] = 1;
            } else { 
                criteriaMatrix[leftCriteria][rightCriteria] = (sliderValue * -1) + 1;
                criteriaMatrix[rightCriteria][leftCriteria] = 1 / ((sliderValue * -1) + 1);
            }
        }
        criteriaMatrix['sum']['flightPricing'] = criteriaMatrix['flightPricing']['flightPricing'] 
                                                + criteriaMatrix['cityRating']['flightPricing'] 
                                                + criteriaMatrix['cityPricing']['flightPricing'] 
                                                + criteriaMatrix['visitFrequency']['flightPricing'];

        criteriaMatrix['sum']['cityRating']    = criteriaMatrix['flightPricing']['cityRating'] 
                                                + criteriaMatrix['cityRating']['cityRating'] 
                                                + criteriaMatrix['cityPricing']['cityRating'] 
                                                + criteriaMatrix['visitFrequency']['cityRating'] ;

        criteriaMatrix['sum']['cityPricing']   = criteriaMatrix['flightPricing']['cityPricing'] 
                                                + criteriaMatrix['cityRating']['cityPricing'] 
                                                + criteriaMatrix['cityPricing']['cityPricing'] 
                                                + criteriaMatrix['visitFrequency']['cityPricing'];

        criteriaMatrix['sum']['visitFrequency']= criteriaMatrix['flightPricing']['visitFrequency'] 
                                                + criteriaMatrix['cityRating']['visitFrequency'] 
                                                + criteriaMatrix['cityPricing']['visitFrequency'] 
                                                + criteriaMatrix['visitFrequency']['visitFrequency'] ;

        window.normalizedMatrix = {
            "flightPricing" : {
                "flightPricing" : null,
                "cityRating" : null,
                "cityPricing" : null,
                "visitFrequency" : null
            },
            "cityRating" : {
                "flightPricing" : null,
                "cityRating" : null,
                "cityPricing" : null,
                "visitFrequency" : null
            },
            "cityPricing" : {
                "flightPricing" : null,
                "cityRating" : null,
                "cityPricing" : null,
                "visitFrequency" : null
            },
            "visitFrequency" : {
                "flightPricing" : null,
                "cityRating" : null,
                "cityPricing" : null,
                "visitFrequency" : null
            }
        }

        normalizedMatrix['flightPricing']['flightPricing'] = criteriaMatrix['flightPricing']['flightPricing'] / 
                                                             criteriaMatrix['sum']['flightPricing'];
        normalizedMatrix['flightPricing']['cityRating'] = criteriaMatrix['flightPricing']['cityRating'] / 
                                                             criteriaMatrix['sum']['cityRating'];
        normalizedMatrix['flightPricing']['cityPricing'] = criteriaMatrix['flightPricing']['cityPricing'] / 
                                                             criteriaMatrix['sum']['cityPricing'];
        normalizedMatrix['flightPricing']['visitFrequency'] = criteriaMatrix['flightPricing']['visitFrequency'] / 
                                                             criteriaMatrix['sum']['visitFrequency'];
        normalizedMatrix['cityRating']['flightPricing']    = criteriaMatrix['cityRating']['flightPricing'] / 
                                                             criteriaMatrix['sum']['flightPricing'];
        normalizedMatrix['cityRating']['cityRating'] = criteriaMatrix['cityRating']['cityRating'] / 
                                                             criteriaMatrix['sum']['cityRating'];
        normalizedMatrix['cityRating']['cityPricing'] = criteriaMatrix['cityRating']['cityPricing'] / 
                                                             criteriaMatrix['sum']['cityPricing'];
        normalizedMatrix['cityRating']['visitFrequency'] = criteriaMatrix['cityRating']['visitFrequency'] / 
                                                             criteriaMatrix['sum']['visitFrequency'];
        normalizedMatrix['cityPricing']['flightPricing'] = criteriaMatrix['cityPricing']['flightPricing'] / 
                                                             criteriaMatrix['sum']['flightPricing'];
        normalizedMatrix['cityPricing']['cityRating'] = criteriaMatrix['cityPricing']['cityRating'] / 
                                                             criteriaMatrix['sum']['cityRating'];
        normalizedMatrix['cityPricing']['cityPricing'] = criteriaMatrix['cityPricing']['cityPricing'] / 
                                                             criteriaMatrix['sum']['cityPricing'];
        normalizedMatrix['cityPricing']['visitFrequency'] = criteriaMatrix['cityPricing']['visitFrequency'] / 
                                                             criteriaMatrix['sum']['visitFrequency'];
        normalizedMatrix['visitFrequency']['flightPricing']    = criteriaMatrix['visitFrequency']['flightPricing'] / 
                                                             criteriaMatrix['sum']['flightPricing'];
        normalizedMatrix['visitFrequency']['cityRating'] = criteriaMatrix['visitFrequency']['cityRating'] / 
                                                             criteriaMatrix['sum']['cityRating'];
        normalizedMatrix['visitFrequency']['cityPricing'] = criteriaMatrix['visitFrequency']['cityPricing'] / 
                                                             criteriaMatrix['sum']['cityPricing'];
        normalizedMatrix['visitFrequency']['visitFrequency'] = criteriaMatrix['visitFrequency']['visitFrequency'] / 
                                                             criteriaMatrix['sum']['visitFrequency'];

        window.eigenVector = {
            "flightPricing" : (normalizedMatrix['flightPricing']['flightPricing'] + normalizedMatrix['flightPricing']['cityRating'] + normalizedMatrix['flightPricing']['cityPricing'] + normalizedMatrix['flightPricing']['visitFrequency']) / 4,
            "cityRating" : (normalizedMatrix['cityRating']['flightPricing'] + normalizedMatrix['cityRating']['cityRating'] + normalizedMatrix['cityRating']['cityPricing'] + normalizedMatrix['cityRating']['visitFrequency']) / 4,
            "cityPricing" : (normalizedMatrix['cityPricing']['flightPricing'] + normalizedMatrix['cityPricing']['cityRating'] + normalizedMatrix['cityPricing']['cityPricing'] + normalizedMatrix['cityPricing']['visitFrequency']) / 4,
            "visitFrequency" : (normalizedMatrix['visitFrequency']['flightPricing'] + normalizedMatrix['visitFrequency']['cityRating'] + normalizedMatrix['visitFrequency']['cityPricing'] + normalizedMatrix['visitFrequency']['visitFrequency']) / 4
        };

        window.ax = {
            'flightPricing' : criteriaMatrix['flightPricing']['flightPricing'] * eigenVector['flightPricing'] +
                              criteriaMatrix['flightPricing']['cityRating'] * eigenVector['cityRating'] +
                              criteriaMatrix['flightPricing']['cityPricing'] * eigenVector['cityPricing'] +
                              criteriaMatrix['flightPricing']['visitFrequency'] * eigenVector['visitFrequency'],
            'cityRating' : criteriaMatrix['cityRating']['flightPricing'] * eigenVector['flightPricing'] +
                              criteriaMatrix['cityRating']['cityRating'] * eigenVector['cityRating'] +
                              criteriaMatrix['cityRating']['cityPricing'] * eigenVector['cityPricing'] +
                              criteriaMatrix['cityRating']['visitFrequency'] * eigenVector['visitFrequency'],
            'cityPricing' : criteriaMatrix['cityPricing']['flightPricing'] * eigenVector['flightPricing'] +
                              criteriaMatrix['cityPricing']['cityRating'] * eigenVector['cityRating'] +
                              criteriaMatrix['cityPricing']['cityPricing'] * eigenVector['cityPricing'] +
                              criteriaMatrix['cityPricing']['visitFrequency'] * eigenVector['visitFrequency'],
            'visitFrequency' : criteriaMatrix['visitFrequency']['flightPricing'] * eigenVector['flightPricing'] +
                              criteriaMatrix['visitFrequency']['cityRating'] * eigenVector['cityRating'] +
                              criteriaMatrix['visitFrequency']['cityPricing'] * eigenVector['cityPricing'] +
                              criteriaMatrix['visitFrequency']['visitFrequency'] * eigenVector['visitFrequency'],
        };

        window.lambdaMax = {
            'flightPricing' : ax['flightPricing'] / eigenVector['flightPricing'],
            'cityRating' : ax['cityRating'] / eigenVector['cityRating'],
            'cityPricing' : ax['cityPricing'] / eigenVector['cityPricing'],
            'visitFrequency' : ax['visitFrequency'] / eigenVector['visitFrequency']
        };

        window.avarageLambdaMax = (lambdaMax['flightPricing'] + lambdaMax['cityRating'] + lambdaMax['cityPricing'] + lambdaMax['visitFrequency']) / 4;

        window.consistencyIndex = (avarageLambdaMax - 4) / 3;
        window.randomConsistencyIndex = 0.90;
        window.consistencyRatio = consistencyIndex / randomConsistencyIndex;

        if(consistencyRatio < 0.1)
            window.consistent = true;
        else
            window.consistent = false;
    }

    function updateRankingInfo(values, handle, criteriaNumber) {
        var sliderValue = parseInt(values[handle]);
        var sliderTextElement = document.getElementById("criteria" + criteriaNumber);

        if (sliderValue < 0){
            sliderTextElement.innerHTML =  "times less important than ";
            document.getElementById('valueSlider' + criteriaNumber).innerHTML = (sliderValue * (-1));
        } else if (sliderValue == 0){
            sliderTextElement.innerHTML =  "equal to ";
            document.getElementById('valueSlider' + criteriaNumber).innerHTML = "";
        } else {
            sliderTextElement.innerHTML =  "times more important than ";
            document.getElementById('valueSlider' + criteriaNumber).innerHTML = sliderValue;
        }

        updateCriteriaMatrix(criteriaNumber, sliderValue);
    }

